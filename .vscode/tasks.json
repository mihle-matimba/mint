{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "npm: dev",
      "type": "shell",
      "command": "npm run dev",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": []
    }
    ,
    {
      "label": "npm: install",
      "type": "shell",
      "command": "npm install",
      "problemMatcher": []
    }
    {
      "label": "npm: start-api",
      "type": "shell",
      "command": "npm run start:api",
      "problemMatcher": []
    }
    ,
    {
      "label": "git: push",
      "type": "shell",
      "command": "git push origin main",
      "problemMatcher": []
    }
    ,
    {
      "label": "git: add",
      "type": "shell",
      "command": "git add -A",
      "problemMatcher": []
    }
    ,
    {
      "label": "git: commit",
      "type": "shell",
      "command": "cmd /c \"git commit -m \"Allow profile creation with user token and logging\" || echo no changes to commit\"",
      "problemMatcher": []
    }
    ,
    {
      "label": "git: status",
      "type": "shell",
      "command": "git status --porcelain",
      "problemMatcher": []
    }
    ,
    {
      "label": "git: commit-and-push",
      "type": "shell",
      "command": "git add -A && git commit -m \"Allow profile creation with user token and logging\" || echo 'no changes to commit' && git push origin main",
      "problemMatcher": []
    }
  ]
}
